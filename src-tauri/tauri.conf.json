{
  "productName": "Smoothie",
  "version": "0.1.0",
  "identifier": "com.smoothie.app",
  "build": {
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:3000"
  },
  "app": {
    "windows": [
      {
        "title": "Smoothie",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["msi", "dmg", "deb"]
    ,"icon": []
  }
}
